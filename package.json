{
  "name": "ant-design-pro",
  "version": "5.0.0-beta.2",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.0.19",
    "@ant-design/pro-form": "^1.3.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.9.16",
    "@types/echarts": "^4.9.3",
    "@types/jsonp": "^0.2.0",
    "@types/ol": "^6.4.2",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-resizable": "^1.7.2",
    "@umijs/route-utils": "^1.0.33",
    "ahooks": "^2.9.3",
    "antd": "^4.10.2",
    "classnames": "^2.2.6",
    "echarts": "^5.0.1",
    "echarts-for-react": "^3.0.1",
    "echarts-gl": "^2.0.1",
    "lodash": "^4.17.11",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "moment": "^2.25.3",
    "node-uuid": "^1.4.8",
    "ol": "^6.5.0",
    "omit.js": "^2.0.2",
    "qs": "^6.9.0",
    "react": "^17.0.0",
    "react-collapsed": "^3.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-grid-layout": "^1.2.4",
    "react-helmet-async": "^1.0.4",
    "react-json-view": "^1.21.3",
    "react-loadable": "^5.5.0",
    "umi": "^3.3.3",
    "umi-request": "^1.0.8",
    "unstated-next": "^1.1.0",
    "use-merge-value": "^1.0.1",
    "wangeditor": "^4.6.9"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/node-uuid": "0.0.28",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-grid-layout": "^1.1.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-loadable": "^5.5.4",
    "@umijs/fabric": "^2.3.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.0-rc.6",
    "@umijs/preset-react": "^1.7.4",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "husky": "^6.0.0",
    "jsdom-global": "^3.0.2",
    "jsonp": "^0.2.1",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.0.0",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
